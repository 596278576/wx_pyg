.swiper {
  height: 340rpx;
}
.swiper navigator image {
  width: 100%;
  height: 340rpx;
}

.menus{
  display: flex;
  justify-content: space-around;
  align-content: center;
  margin: 10rpx 0;
}
.menus image{
  width: 128rpx;
  height: 140rpx;
}
.floor-top{
  background: #f4f4f4;
  padding: 10rpx 10rpx 0 10rpx;
}
.floor-top image{
  width: 100%;
  height: 60rpx;
}
.floor-container{
  padding: 15rpx;
  display: flex;
  justify-content: space-between;
}
.floor-left image{
  width: 232rpx;
  height: 386rpx;
}
.floor-right{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: stretch; 
}
.floor-right image{
  margin-left: 13rpx;
  width: 232rpx;
  height: 188rpx;
}
.floor:nth-child(5) .floor-right image,.floor:nth-child(6) .floor-right image{
  margin-left: 13rpx;
  width: 193rpx;
  height: 188rpx;
}
.floor:nth-child(5) .floor-right image:nth-child(3n+1),.floor:nth-child(6) .floor-right image:nth-child(3n+1){
    width: 273rpx;
}
.toTop{
  width: 90rpx;
  height: 90rpx;
  /* background: #eee; */
  background: rgba(255, 255, 255, 0.5);
  border-radius: 55rpx;
  text-align: center;
  position: fixed;
  right: 20rpx;
  bottom: 20rpx;
}